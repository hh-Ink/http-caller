{
    "name": "hhink/http-caller",
    "description": "http caller",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "guzzlehttp",
        "http"
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "psr/log": "^1.0|^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Hhink\\HttpCaller\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Msmm\\HyperfHttpBase\\ConfigProvider"
        }
    }
}
